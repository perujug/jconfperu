{
  "version": 2,
  "name": "jconfperu",
  "builds": [
    {
      "src": "_site/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/_site/$1"
    },
    {
      "src": "/",
      "dest": "/_site/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "github": {
    "enabled": false
  }
}

